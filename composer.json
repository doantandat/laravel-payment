{
    "name": "datdoan/laravel-payment",
    "description": "Laravel Payment Library for integrating VNPay, Momo, ZaloPay and more.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "datdoan",
            "email": "youremail@example.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Datdoan\\LaravelPayment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datdoan\\LaravelPayment\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datdoan\\LaravelPayment\\LaravelPaymentServiceProvider"
            ],
            "aliases": {
                "LaravelPayment": "Datdoan\\LaravelPayment\\Facades\\LaravelPayment"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    }
}
